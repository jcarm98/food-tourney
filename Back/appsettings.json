{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "HttpsFromPem": {
        "Url": "https://localhost:5001",
        "Certificate": {
          "Path": "/etc/letsencrypt/live/foodtourney.app/cert.pem",
          "KeyPath": "/etc/letsencrypt/live/foodtourney.app/privkey.pem"
        }
      }
    }
  }
}
